<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iplay.jsreview">

    <!-- Bmob权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:name="com.iplay.jsreview.commons.base.system.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/r_512px"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.iplay.jsreview.main.AppStartActivity"
            android:theme="@style/Theme.AppStartLoad">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.iplay.jsreview.main.MainActivity" />
        <activity android:name="com.iplay.jsreview.review.view.ListActivity" />
        <activity android:name="com.iplay.jsreview.review.view.DetailActivity" />
        <activity android:name="com.iplay.jsreview.review.view.FavActivity" />
        <activity android:name="com.iplay.jsreview.setting.view.SuggestActivity" />
        <activity android:name="com.iplay.jsreview.setting.view.AboutActivity" />
        <activity android:name="com.iplay.jsreview.setting.view.AuthorActivity" />
        <activity android:name="com.iplay.jsreview.setting.view.PermissionsActivity" />
        <activity android:name="com.iplay.jsreview.setting.view.SettingCacheActivity" />
        <activity android:name="com.iplay.jsreview.setting.view.CreateTableActivity" />
        <activity android:name="com.iplay.jsreview.setting.view.VersionNoteActivity"/>
    </application>

</manifest>
